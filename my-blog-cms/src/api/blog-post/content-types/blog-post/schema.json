{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "Blog Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "category": {
      "type": "string"
    },
    "Title": {
      "type": "string"
    },
    "excerpt": {
      "type": "text"
    },
    "date": {
      "type": "datetime"
    },
    "blog_post": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::blog-post.blog-post"
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
